    "version": "0.1",

    // OpenEnclave specific values
    "Debug": 1,
    "KernelMemSize": "4m",
    "StackMemSize": "256k",
    "NumUserThreads": 2,
    "ProductID": 1,
    "SecurityVersion": 1,

    // LibOS specific values
