    "HostApplicationParameters": true,
    "EnvironmentVariables": ["ENC-ENVP-1=Enclave_envp_1", "ENC-ENVP-2=Enclave_envp_1"],
    "HostEnvironmentVariables": ["TESTNAME"]
}
