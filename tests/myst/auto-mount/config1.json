{
    // Mystikos configuration version number
    "version": "0.1",

    // OpenEnclave specific values
    "Debug": 1,
    "StackMemSize": "256k",
    "NumUserThreads": 2,
    "ProductID": 1,
    "SecurityVersion": 1,

    // Mystikos specific values
    "MemorySize": "30m",
    "ApplicationPath": "/bin/hello",
    "Hostname": "test1",
    "ApplicationParameters": ["Enclave-red", "Enclave-blue", "Enclave-green", "Enclave-yellow", "Enclave-pink"],
    "HostApplicationParameters": true,
    "EnvironmentVariables": ["ENC-ENVP-1=Enclave_envp_1", "ENC-ENVP-2=Enclave_envp_1"],
    "HostEnvironmentVariables": ["TESTNAME"],

    "Mount": [
        {
            "Target": "/hostfs-target",
            "Type": "hostfs"
        },
        {
            "Target": "/targetfile1",
            "Type": "hostfs"
        },
        {
            "Target": "/targetfile2",
            "Type": "hostfs"
        },
        {
            "Target": "/ext2-target",
            "Type": "ext2",
            "Flags": [],
            "RootHash": null,
            "PublicKey": null
        },
        {
            "Target": "/ramfs-target",
            "Type": "ramfs"
        }
    ]
}
